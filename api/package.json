{
  "name": "panaderia-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/main.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "node --loader ts-node/esm prisma/seed.ts",
    "openapi:gen": "node --loader ts-node/esm scripts/generate-openapi.ts",
    "openapi:gen:dist": "npm run build && node scripts/generate-openapi-runtime.js",
    "docs:validate": "bump deploy --dry-run --doc panaderia-api --token 9LQdK2XJ8m5WbJeEApXp3BKGFe1NgmKq openapi.json",
    "docs:deploy": "bump deploy --doc panaderia-api --token 9LQdK2XJ8m5WbJeEApXp3BKGFe1NgmKq openapi.json",
    "docs:diff": "bump diff --doc panaderia-api --token 9LQdK2XJ8m5WbJeEApXp3BKGFe1NgmKq openapi.json",
    "test": "jest -c jest.config.cjs",
    "test:e2e": "jest -c jest.config.cjs"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.19.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "helmet": "^8.1.0",
    "nestjs-pino": "^4.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^5.19.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.20",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
